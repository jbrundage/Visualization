{
  "private": true,
  "name": "@hpcc-js/test-map",
  "version": "2.1.1",
  "dependencies": {
    "@hpcc-js/common": "^2.11.0",
    "@hpcc-js/dgrid-shim": "^2.0.8",
    "@hpcc-js/map": "^2.2.0",
    "@hpcc-js/test-data": "^2.0.2",
    "@hpcc-js/util": "^2.2.1",
    "chai": "4.2.0",
    "es6-promise": "4.2.4",
    "mocha": "5.2.0",
    "simulate-event": "1.4.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.6",
    "@types/mocha": "5.2.5",
    "cpx": "1.5.0",
    "css-loader": "1.0.0",
    "file-loader": "1.1.5",
    "mocha-chrome": "1.1.0",
    "npm-run-all": "4.1.5",
    "rimraf": "2.6.2",
    "source-map-loader": "0.2.1",
    "style-loader": "0.22.1",
    "typescript": "3.3.3",
    "webpack": "4.16.5",
    "webpack-cli": "3.1.0"
  },
  "scripts": {
    "clean": "rimraf lib* types dist",
    "copy-resources": "cpx ./node_modules/@hpcc-js/dgrid-shim/dist/dojo/resources/* ./dist/dojo/resources",
    "compile-es6": "tsc --module es6 --outDir ./lib-es6",
    "compile-es6-watch": "npm run compile-es6 -- -w",
    "bundle": "webpack",
    "bundle-watch": "npm run bundle -- -w",
    "watch": "run-p compile-es6-watch bundle-watch",
    "build": "npm run compile-es6 && npm run bundle && npm run copy-resources",
    "test-skip": "mocha-chrome ./test.html"
  }
}